    "eslint": "^5.16.0",
    "eslint-config-precure": "^4.15.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "redux": "^4.0.1",
